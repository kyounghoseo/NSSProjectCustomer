<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-
mapper.dtd">
<mapper namespace="com.nss.customer.menlifestyledao.MenLifeStyleMapper">
	<select id="lifestyleList" parameterType="com.nss.customer.menlifestylevo.MenLifeStyleVO"
		resultType="com.nss.customer.menlifestylevo.MenLifeStyleVO">
		SELECT productNo, productName, productCategory,
		productPrice
		from product_tb
		where productCategory = '라이프 스타일M'
	</select>
</mapper>