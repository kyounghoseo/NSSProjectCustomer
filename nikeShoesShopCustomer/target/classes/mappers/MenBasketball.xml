<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.nss.customer.menbasketballdao.MenBasketballDao">
 
 <select id="basketballList" parameterType="com.nss.customer.menbasketballvo.MenBasketballVO"
		resultType="com.nss.customer.menbasketballvo.MenBasketballVO">
		SELECT productNo, productName, productCategory,
		productPrice
		from product_tb
		where productCategory = '농구화M'
	</select>
 
 
 </mapper>