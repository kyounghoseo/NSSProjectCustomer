<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nss.customer.mensoccerdao.MenSoccerDao">
	<select id="soccerList" parameterType="com.nss.customer.mensoccervo.MenSoccerVO"
		resultType="com.nss.customer.mensoccervo.MenSoccerVO">
		SELECT productNo, productName, productCategory,
		productPrice
		from product_tb
		where productCategory = '축구화M'
		</select>  
</mapper>